import java.util.ArrayList;
import java.util.List;

public class MixedTypeSort {

    public static void main(String[] args) {
        List<Object> mixedList = new ArrayList<>();
        mixedList.add("apple");
        mixedList.add(20);
        mixedList.add("banana");
        mixedList.add(10);
        mixedList.add(5);
        mixedList.add("100");
        bubbleSortMixedList(mixedList);
        System.out.println(mixedList);
    }

    public static void bubbleSortMixedList(List<Object> list) {
        int n = list.size();
        for (int i = 0; i < n - 1; i++) {
            for (int j = 0; j < n - i - 1; j++) {
                Object o1 = list.get(j);
                Object o2 = list.get(j + 1);
                if (o1 instanceof String && o2 instanceof Integer) {
                    swap(list, j, j + 1);
                }
                else if (o1 instanceof Integer && o2 instanceof Integer) {
                    if ((Integer) o1 > (Integer) o2) {
                        swap(list, j, j + 1);
                    }
                }
                
                else if (o1 instanceof String && o2 instanceof String) {
                    if (((String) o1).compareTo((String) o2) > 0) {
                        swap(list, j, j + 1);
                    }
                }
            }
        }
    }
    public static void swap(List<Object> list, int i, int j) {
        Object temp = list.get(i);
        list.set(i, list.get(j));
        list.set(j, temp);
    }
}
